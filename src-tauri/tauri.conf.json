{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "tiktok-matrix",
    "version": "1.0.5"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "app": {
        "all": true,
        "show": true,
        "hide": true
      }
    },
    "bundle": {
      "resources": [
        "bin/dist",
        "bin/PaddleOCR-json_v.1.3.1",
        "bin/platform-tools",
        "bin/raw",
        "bin/tiktok-agent.exe",
        "bin/tiktok-server.exe",
        "bin/scrcpy-agent.exe"
      ],
      "active": true,
      "targets": "msi",
      "identifier": "com.niostack.tt",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "tiktok-matrix",
        "width": 800,
        "height": 600
      }
    ]
  }
}