{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "tiktok-matrix",
    "version": "1.0.6"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "app": {
        "all": true,
        "show": true,
        "hide": true
      }
    },
    "bundle": {
      "resources": [
        "bin/dist",
        "bin/platform-tools",
        "bin/raw",
        "bin/tiktok-agent.exe",
        "bin/tiktok-server.exe",
        "bin/scrcpy-agent.exe"
      ],
      "active": true,
      "targets": [
        "msi",
        "updater"
      ],
      "identifier": "com.niostack.tt",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "tiktok-matrix",
        "width": 800,
        "height": 600
      }
    ],
    "updater": {
      "active": true,
      "dialog": true,
      "endpoints": [
        "https://raw.githubusercontent.com/niostack/tiktok-matrix/main/update.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IENCNzMyMUE4ODZENTcwOEEKUldTS2NOV0dxQ0Z6eStkaXhiMERsc3c4MWF5Y1R0YUp6Wlo1VE5saWt5bXlVYmV3ejVSQW5PR1cK",
      "windows": {
        "installMode": "passive"
      }
    }
  }
}